{
    "name": "ol-contextmenu",
    "version": "5.4.1",
    "description": "Custom Context Menu for Openlayers",
    "type": "module",
    "main": "./dist/ol-contextmenu.umd.cjs",
    "jsdelivr": "./dist/ol-contextmenu.iife.js",
    "module": "./dist/ol-contextmenu.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/ol-contextmenu.d.ts",
                "default": "./dist/ol-contextmenu.js"
            }
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "style": "./*"
        },
        "./ol-contextmenu.css": "./dist/ol-contextmenu.css"
    },
    "files": [
        "./dist"
    ],
    "types": "./dist/ol-contextmenu.d.ts",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/ol-contextmenu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/ol-contextmenu.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/ol-contextmenu/issues"
    },
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    },
    "scripts": {
        "dev": "vite",
        "build": "rimraf ./dist && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest",
        "prepare": "husky",
        "release": "release-it --no-npm.publish"
    },
    "devDependencies": {
        "@playwright/test": "^1.45.3",
        "@types/node": "^22.0.2",
        "auto-changelog": "^2.4.0",
        "eslint-config-jwalker": "^11.0.2",
        "husky": "^9.1.4",
        "kill-port": "^2.0.1",
        "ol": "^10.0.0",
        "release-it": "^17.6.0",
        "rimraf": "^6.0.1",
        "sass": "^1.77.8",
        "typescript": "^5.5.4",
        "vite": "^5.3.5",
        "vite-plugin-banner": "^0.7.1",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^2.0.5"
    },
    "peerDependencies": {
        "ol": "> 7.x <= 9.x"
    },
    "dependencies": {
        "tiny-emitter": "^2.1.0"
    }
}
