{
    "name": "ol-contextmenu",
    "version": "5.4.1",
    "description": "Custom Context Menu for Openlayers",
    "type": "module",
    "main": "./dist/ol-contextmenu.umd.cjs",
    "jsdelivr": "./dist/ol-contextmenu.iife.js",
    "module": "./dist/ol-contextmenu.js",
    "exports": {
        ".": {
            "types": "./dist/ol-contextmenu.d.ts",
            "import": "./dist/ol-contextmenu.js"
        },
        "./ol-contextmenu.css": "./dist/ol-contextmenu.css"
    },
    "files": [
        "./dist"
    ],
    "types": "./dist/ol-contextmenu.d.ts",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/ol-contextmenu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/ol-contextmenu.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/ol-contextmenu/issues"
    },
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    },
    "scripts": {
        "dev": "vite",
        "build": "rimraf ./dist && vite build",
        "preview": "vite preview",
        "lint": "eslint . --cache",
        "lint:fix": "npm run lint -- --fix",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "prepare": "husky",
        "release": "release-it --no-npm.publish"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "@vitest/browser-webdriverio": "^4.0.8",
        "@vitest/coverage-v8": "^4.0.8",
        "@vitest/ui": "^4.0.8",
        "auto-changelog": "^2.5.0",
        "eslint-config-jwalker": "^12.0.0",
        "husky": "^9.1.7",
        "jsdom": "^27.1.0",
        "ol": "^10.7.0",
        "release-it": "^19.0.6",
        "sass": "^1.93.3",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vite-plugin-banner": "^0.8.1",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.8",
        "webdriverio": "^9.20.0"
    },
    "peerDependencies": {
        "ol": "> 7.x <= 9.x"
    },
    "dependencies": {
        "tiny-emitter": "^2.1.0"
    }
}
