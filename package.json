{
    "name": "ol-contextmenu",
    "version": "5.5.0",
    "description": "Custom Context Menu for Openlayers",
    "type": "module",
    "main": "./dist/ol-contextmenu.umd.cjs",
    "jsdelivr": "./dist/ol-contextmenu.iife.js",
    "module": "./dist/ol-contextmenu.js",
    "exports": {
        ".": {
            "types": "./dist/ol-contextmenu.d.ts",
            "import": "./dist/ol-contextmenu.js"
        }
    },
    "files": [
        "./dist"
    ],
    "types": "./dist/ol-contextmenu.d.ts",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/ol-contextmenu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/ol-contextmenu.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/ol-contextmenu/issues"
    },
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    },
    "scripts": {
        "dev": "vite",
        "build": "premove ./dist && vite build",
        "preview": "vite preview",
        "lint": "eslint . --cache",
        "lint:fix": "npm run lint -- --fix",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "prepare": "husky",
        "release": "release-it --no-npm.publish"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "@vitest/browser-webdriverio": "^4.0.16",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "auto-changelog": "^2.5.0",
        "eslint-config-jwalker": "^12.1.0",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "ol": "^10.7.0",
        "premove": "^4.0.0",
        "release-it": "^19.2.2",
        "sass": "^1.97.1",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-banner": "^0.8.1",
        "vite-plugin-css-injected-by-js": "^3.5.2",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.16",
        "webdriverio": "^9.23.0"
    },
    "peerDependencies": {
        "ol": "> 7.x <= 10.x"
    },
    "dependencies": {
        "tiny-emitter": "^2.1.0"
    }
}
